package jungle.cave;

public class Tiger {
	void roar() {
		System.out.println("Tiger is roaring...");
	}
}
class Lion {
	void roar() {
		System.out.println("Lion is roaring...");
	}
}
class Bear {
	void climb() {
		System.out.println("Bear is climbing...");
	}
	void hatchHoney() {
		System.out.println("Bear is Hatching honey...");
	}
}
