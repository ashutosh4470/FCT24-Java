
public class StudentTest {

	public static void main(String[] args) {

	}
}

class Student{
	int rollNo;
	String name;
	String subject1;
	String subject2;
	int subjectOneMarks;
	int subjectTwoMarks;
	int totalMarks;

	void subjectOneTest() {
		subjectOneMarks+=80;
		totalMarks+=subjectOneMarks;
	}
	void subjectTwoTest() {
		subjectTwoMarks+=70;
		totalMarks+=subjectTwoMarks;
	}
	void displayStudentInfo() {
		System.out.println("Student Roll No :"+rollNo);
		System.out.println("   Student Name :"+rollNo);
		System.out.println(subject1 +" Marks : "+subjectOneMarks);
		System.out.println(subject1 +" Marks : "+subjectTwoMarks);
	}
}